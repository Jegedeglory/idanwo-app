{
  "expo": {
    "name": "quiz-app",
    "slug": "quiz-app",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/WhiteIqSplash.png",
    "userInterfaceStyle": "#FDF0DC",
    "newArchEnabled": true,
    "splash": {
      "image": "./assets/WhiteIqSplash.png",
      "backgroundColor": "#FDF0DC",
      "resizeMode": "contain"
    },
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#FDF0DC"
      },
      "splash": {
        "image": "./assets/WhiteIqSplash.png",
        "backgroundColor": "#FDF0DC",
        "resizeMode": "contain"
      }

    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "plugins": [
      "expo-router",
      [
        "expo-splash-screen",
        {
          "image": "./assets/WhiteIqSplash.png",
          "backgroundColor": "#FDF0DC",
          "imageResizeMode": "contain",
          "imageWidth": "200",

        }
      ]
    ]
  }
}